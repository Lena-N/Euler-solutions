package euler;

public class Euler {

    public static void main(String[] args) {
        double sum1=0;
        double sum2=0;
      for (double i=1;i<=100;i++){
          sum1+=Math.pow(i, 2);
      }
      for(double j=1;j<=100;j++){
          sum2+=j;
      }
      double sol=Math.pow(sum2, 2);
        System.out.println(sol-sum1);
    }

    
        
 }
