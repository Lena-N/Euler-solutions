package euler;

public class Euler {

    public static void main(String[] args) {
        int num=1;
        while(num>0){
            if(div(num)){
                System.out.println(num);
                break;
            }
        num++;
        }
    }
 static boolean div(int num){
     for (int i=1;i<21;i++){
         if(num%i!=0)
             return false;
     } 
             return true;
     
 }
   
    
        
 }
